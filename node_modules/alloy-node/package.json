  "dependencies": {
    "axios": "^0.21.1",
    "mongodb": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(js?|mjs?)$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
